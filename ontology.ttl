@prefix ex: <http://example.org/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ex:Point a rdfs:Class .

ex:hasDistance a owl:DatatypeProperty ;
    rdfs:domain ex:Chair,
        ex:Suitcase ;
    rdfs:range xsd:integer .

ex:hasIdentifier a owl:DatatypeProperty ;
    rdfs:domain ex:Chair,
        ex:Suitcase ;
    rdfs:range xsd:string .

ex:hasPoint a owl:DatatypeProperty ;
    rdfs:domain ex:Chair,
        ex:Suitcase ;
    rdfs:range ex:Point .

ex:myChair_6969 a ex:Chair ;
    ex:hasDistance 5 ;
    ex:hasIdentifier "chair1" ;
    ex:hasPoint ex:myPoint_6969.

ex:myChair_420 a ex:Chair ;
    ex:hasDistance 5 ;
    ex:hasIdentifier "chair1" ;
    ex:hasPoint ex:myPoint_420 .

ex:x a rdf:Property ;
    rdfs:domain ex:Point ;
    rdfs:range xsd:integer .

ex:y a rdf:Property ;
    rdfs:domain ex:Point ;
    rdfs:range xsd:integer .

ex:myPoint_6969 a ex:Point ;
    ex:x 10.255 ;
    ex:y 20.2525 .
ex:myPoint_420 a ex:Point ;
    ex:x 10 ;
    ex:y 20 .

ex:Suitcase a owl:Class .

ex:Chair a owl:Class .

IF {
  ?individual1 rdf:type ?class .
  ?individual1 ex:hasPoint ?point .
  ?individual2 rdf:type ?class .
  ?individual2 ex:hasPoint ?point .
  FILTER(?individual1 != ?individual2)
}
THEN {
  ?individual1 owl:sameAs ?individual2
}