<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF
  xmlns:owl="http://www.w3.org/2002/07/owl#"
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xmlns:ex="http://example.org/"
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
>
  <ex:Chair rdf:about="http://example.org/myChair">
    <ex:hasDistance rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</ex:hasDistance>
    <ex:hasIdentifier>chair1</ex:hasIdentifier>
    <ex:hasPoint>
      <ex:Point rdf:about="http://example.org/myPoint">
        <ex:x rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">10</ex:x>
        <ex:y rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">20</ex:y>
      </ex:Point>
    </ex:hasPoint>
  </ex:Chair>
  <owl:DatatypeProperty rdf:about="http://example.org/hasX">
    <rdfs:domain rdf:resource="http://example.org/Chair"/>
    <rdfs:domain rdf:resource="http://example.org/Suitcase"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
  </owl:DatatypeProperty>
  <rdf:Property rdf:about="http://example.org/y">
    <rdfs:domain rdf:resource="http://example.org/Point"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
  </rdf:Property>
  <owl:DatatypeProperty rdf:about="http://example.org/hasDistance">
    <rdfs:domain rdf:resource="http://example.org/Chair"/>
    <rdfs:domain rdf:resource="http://example.org/Suitcase"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/hasY">
    <rdfs:domain rdf:resource="http://example.org/Chair"/>
    <rdfs:domain rdf:resource="http://example.org/Suitcase"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/hasIdentifier">
    <rdfs:domain rdf:resource="http://example.org/Chair"/>
    <rdfs:domain rdf:resource="http://example.org/Suitcase"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:about="http://example.org/hasPoint">
    <rdfs:domain rdf:resource="http://example.org/Chair"/>
    <rdfs:domain rdf:resource="http://example.org/Suitcase"/>
    <rdfs:range rdf:resource="http://example.org/Point"/>
  </owl:DatatypeProperty>
  <rdf:Property rdf:about="http://example.org/x">
    <rdfs:domain rdf:resource="http://example.org/Point"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
  </rdf:Property>
  <owl:Class rdf:about="http://example.org/Suitcase"/>
  <rdfs:Class rdf:about="http://example.org/Point"/>
  <owl:Class rdf:about="http://example.org/Chair"/>
</rdf:RDF>
